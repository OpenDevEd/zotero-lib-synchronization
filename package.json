    "@prisma/client": "^5.3.1",
    "@sentry/node": "^7.63.0",
    "@supabase/supabase-js": "^2.46.1",
    "@types/node": "^14.14.14",
    "ajv": "^6.12.6",
    "argparse": "^2.0.1",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "dotenv": "^8.6.0",
    "drizzle-orm": "^0.35.3",
