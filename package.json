    "sqlite3": "^5.0.2",
    "standard-version": "^9.3.1",
    "string-similarity": "^4.0.4",
    "sugar": "^2.0.6",
    "ts-dotenv": "^0.8.3",
    "tslib": "^2.2.0",
    "uuid": "^11.0.2",
    "winston": "^3.6.0",
    "ws": "^8.14.2",
    "xml-escape": "^1.1.0",
    "xml-js": "^1.6.11"
