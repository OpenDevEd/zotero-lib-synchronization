    "http-link-header": "^1.0.3",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.0",
    "node-libcurl": "^3.0.0",
    "object-hash": "^3.0.0",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.1.3",
